{
  "name": "auto-fill-tool",
  "version": "2.4.0",
  "description": "Auto Fill Tool - Chrome extension for automated web form filling with Clean Architecture and DDD",
  "scripts": {
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "build:clean": "npm run clean && npm run build",
    "dev": "webpack --mode development --watch",
    "clean": "rm -rf dist .test-results coverage complexity-report.json dependency-graph.svg",
    "test": "NODE_OPTIONS=\"--max-old-space-size=2048\" jest --maxWorkers=50%",
    "test:watch": "NODE_OPTIONS=\"--max-old-space-size=2048\" jest --watch --maxWorkers=25%",
    "test:coverage": "NODE_OPTIONS=\"--max-old-space-size=4096\" jest --coverage --testEnvironment=jsdom",
    "test:silent": "NODE_OPTIONS=\"--max-old-space-size=2048\" jest --silent",
    "test:ci": "NODE_OPTIONS=\"--max-old-space-size=4096\" jest --ci --coverage --maxWorkers=1 --testEnvironment=jsdom",
    "test:unit": "jest --testPathPattern='src/' --maxWorkers=50%",
    "test:integration": "jest --testPathPattern='tests/integration' --testEnvironment=jsdom",
    "test:performance": "jest --testPathPattern='tests/performance'",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "lint": "eslint src --ext .ts --max-warnings 500 --cache --cache-location .eslintcache",
    "lint:fix": "eslint src --ext .ts --fix --cache --cache-location .eslintcache",
    "lint:architecture": "eslint src/domain src/usecases --ext .ts --max-warnings 0",
    "lint:report": "eslint src --ext .ts,.tsx -f json -o eslint-report.json",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "type-check:test": "tsc --noEmit -p tsconfig.test.json",
    "type-check:watch": "tsc --noEmit --watch",
    "type-check:all": "npm run type-check && npm run type-check:test",
    "quality": "npm run lint && npm run type-check",
    "quality:fix": "npm run lint:fix && npm run format",
    "validate": "npm run quality && npm run test:silent",
    "dev:check": "npm run lint && npm run test:unit",
    "ci": "npm run quality && npm run test:ci && npm run build",
    "complexity": "eslint src --ext .ts --ignore-pattern '**/__tests__/**' --ignore-pattern '**/*.test.ts' --rule 'complexity: [error, 10]' --rule 'max-depth: [error, 4]' --rule 'max-lines-per-function: [error, {max: 50, skipBlankLines: true, skipComments: true}]'",
    "analyze:circular": "madge --circular --extensions ts src",
    "analyze:deps": "madge --image dependency-graph.svg --extensions ts src",
    "security:audit": "npm audit --audit-level=moderate",
    "security:audit:full": "npm audit --audit-level=low",
    "security:audit:critical": "npm audit --audit-level=critical",
    "security:audit:json": "npm audit --json > security-audit-report.json",
    "security:fix": "npm audit fix",
    "security:fix:force": "npm audit fix --force",
    "security:check": "npm run security:audit && echo '\n✅ Security audit passed'",
    "vendor:check": "echo 'Vendor libraries:' && echo 'Alpine.js:' $(npm list alpinejs --depth=0 2>/dev/null | grep alpinejs) && echo 'Chart.js:' $(npm list chart.js --depth=0 2>/dev/null | grep chart.js)",
    "vendor:update": "npm update alpinejs chart.js && npm run build && echo '\n✅ Vendor libraries updated and built'",
    "prepare": "husky"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@playwright/test": "^1.56.1",
    "@types/chrome": "^0.0.268",
    "@types/jest": "^29.5.14",
    "@types/jsonpath": "^0.2.4",
    "@types/jsonpath-plus": "^5.0.5",
    "@types/node": "^20.12.7",
    "@types/papaparse": "^5.3.16",
    "@types/uuid": "^10.0.0",
    "@types/webextension-polyfill": "^0.12.4",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "autoprefixer": "^10.4.21",
    "axios-mock-adapter": "^2.1.0",
    "babel-loader": "^10.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "cssnano": "^7.1.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^16.2.3",
    "madge": "^7.0.0",
    "mini-css-extract-plugin": "^2.9.4",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.2.0",
    "prettier": "^3.6.2",
    "process": "^0.11.10",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.18",
    "terser-webpack-plugin": "^5.3.9",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5",
    "webpack": "^5.91.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@alpinejs/csp": "^3.15.0",
    "@notionhq/client": "^5.2.1",
    "@types/dompurify": "^3.0.5",
    "alpinejs": "^3.15.1",
    "axios": "^1.12.2",
    "axios-retry": "^4.5.0",
    "chart.js": "^4.5.1",
    "dompurify": "^3.3.0",
    "googleapis": "^164.0.0",
    "jsonpath": "^1.1.1",
    "jsonpath-plus": "^10.3.0",
    "papaparse": "^5.5.3",
    "uuid": "^13.0.0",
    "webextension-polyfill": "^0.12.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
