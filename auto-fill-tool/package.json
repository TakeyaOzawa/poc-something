{
  "name": "auto-fill-tool",
  "version": "2.4.0",
  "description": "Auto Fill Tool - Chrome extension for automated web form filling with Clean Architecture and DDD",
  "scripts": {
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "build:clean": "npm run clean && npm run build",
    "dev": "webpack --mode development --watch",
    "clean": "rm -rf dist .test-results coverage complexity-report.json dependency-graph.svg",
    "test": "NODE_OPTIONS=\"--max-old-space-size=2048\" jest --maxWorkers=50%",
    "test:watch": "NODE_OPTIONS=\"--max-old-space-size=2048\" jest --watch --maxWorkers=25%",
    "test:coverage": "NODE_OPTIONS=\"--max-old-space-size=4096\" jest --coverage --testEnvironment=jsdom",
    "test:silent": "NODE_OPTIONS=\"--max-old-space-size=2048\" jest --silent",
    "test:ci": "NODE_OPTIONS=\"--max-old-space-size=4096\" jest --ci --coverage --maxWorkers=1 --testEnvironment=jsdom",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "lint": "eslint src --ext .ts --max-warnings 500 --cache --cache-location .eslintcache",
    "lint:fix": "eslint src --ext .ts --fix --cache --cache-location .eslintcache",
    "lint:architecture": "eslint src/domain src/usecases --ext .ts --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "type-check:test": "tsc --noEmit -p tsconfig.test.json",
    "type-check:watch": "tsc --noEmit --watch",
    "complexity": "eslint src --ext .ts --ignore-pattern '**/__tests__/**' --ignore-pattern '**/*.test.ts' --rule 'complexity: [error, 10]' --rule 'max-depth: [error, 4]' --rule 'max-lines-per-function: [error, {max: 50, skipBlankLines: true, skipComments: true}]'",
    "analyze:circular": "madge --circular --extensions ts src",
    "analyze:deps": "madge --image dependency-graph.svg --extensions ts src",
    "security:audit": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "vendor:check": "echo 'Vendor libraries:' && echo 'Alpine.js:' $(npm list alpinejs --depth=0 2>/dev/null | grep alpinejs) && echo 'Chart.js:' $(npm list chart.js --depth=0 2>/dev/null | grep chart.js)",
    "vendor:update": "npm update alpinejs chart.js && npm run build && echo '\nâœ… Vendor libraries updated and built'",
    "error:list": "bash scripts/coding-helpers/validate-and-test.sh list",
    "error:reserve": "bash scripts/coding-helpers/validate-and-test.sh reserve",
    "error:validate": "bash scripts/coding-helpers/validate-and-test.sh validate",
    "error:docs": "bash scripts/coding-helpers/validate-and-test.sh generate-docs",
    "hooks:quality-gate": "bash scripts/coding-helpers/quality-gate.sh",
    "hooks:coverage": "bash scripts/coding-helpers/coverage-check.sh",
    "hooks:commit-check": "bash scripts/coding-helpers/git-commit-quality.sh",
    "hooks:pre-commit": "bash scripts/coding-helpers/pre-commit-review.sh",
    "hooks:error-check": "bash scripts/coding-helpers/error-handling-check.sh",
    "hooks:i18n": "bash scripts/coding-helpers/i18n-resource-update.sh",
    "hooks:test": "bash scripts/coding-helpers/run-all-hooks.sh",
    "prepare": "husky"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@playwright/test": "^1.57.0",
    "@types/chrome": "^0.1.32",
    "@types/jest": "^30.0.0",
    "@types/jsonpath": "^0.2.4",
    "@types/jsonpath-plus": "^5.0.5",
    "@types/node": "^25.0.3",
    "@types/papaparse": "^5.5.2",
    "@types/uuid": "^10.0.0",
    "@types/webextension-polyfill": "^0.12.4",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "autoprefixer": "^10.4.23",
    "axios-mock-adapter": "^2.1.0",
    "babel-loader": "^10.0.0",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^7.1.2",
    "cssnano": "^7.1.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "fast-check": "^4.5.2",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "lint-staged": "^16.2.7",
    "madge": "^8.0.0",
    "mini-css-extract-plugin": "^2.9.4",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.2.0",
    "prettier": "^3.7.4",
    "process": "^0.11.10",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.19",
    "terser-webpack-plugin": "^5.3.16",
    "ts-jest": "^29.4.6",
    "ts-loader": "^9.5.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5",
    "webpack": "^5.104.1",
    "webpack-bundle-analyzer": "^5.1.0",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@alpinejs/csp": "^3.15.3",
    "@notionhq/client": "^5.6.0",
    "@types/dompurify": "^3.0.5",
    "alpinejs": "^3.15.3",
    "axios": "^1.13.2",
    "axios-retry": "^4.5.0",
    "chart.js": "^4.5.1",
    "dompurify": "^3.3.1",
    "googleapis": "^169.0.0",
    "jsonpath": "^1.1.1",
    "jsonpath-plus": "^10.3.0",
    "papaparse": "^5.5.3",
    "uuid": "^13.0.0",
    "webextension-polyfill": "^0.12.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
